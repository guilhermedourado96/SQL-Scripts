CREATE FUNCTION [dbo].[fnRemoveCaracteresEspeciais] (@TEXTO VARCHAR(MAX)) RETURNS VARCHAR(MAX) AS
BEGIN
	DECLARE @STRING_TRATADA VARCHAR(MAX)
	SET @STRING_TRATADA = UPPER(@TEXTO)
	    COLLATE sql_latin1_general_cp1250_ci_as
	SET @STRING_TRATADA = @STRING_TRATADA
	    COLLATE sql_latin1_general_cp1251_ci_as
	RETURN @STRING_TRATADA
END
